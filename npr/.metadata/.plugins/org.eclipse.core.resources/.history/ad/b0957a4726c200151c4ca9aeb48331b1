VAR_INPUT
	kihanje : REAL;
	kasljanje : REAL;
	grlobolja : REAL;
	otezano disanje : REAL;
	glavobolja : REAL;
	visina temperature : REAL;
	bol u plucima : REAL;
	krvav ispljuvak : REAL;
	suzenje : REAL;
	promuklost : REAL;
	hripanje : REAL;
	zacepljen nos : REAL;
	gubitak apetita : REAL;
	slabost tijela : REAL;
END_VAR

VAR_OUTPUT
	decision : REAL;
END_VAR

FUZZIFY kihanje
	TERM 
END_FUZZIFY

FUZZIFY kasljanje
	TERM 
END_FUZZIFY

FUZZIFY grlobolja
	TERM 
END_FUZZIFY

FUZZIFY otezano disanje
	TERM 
END_FUZZIFY

FUZZIFY glavobolja
	TERM 
END_FUZZIFY

FUZZIFY visina temperature
	TERM 
END_FUZZIFY

FUZZIFY bol u plucima
	TERM 
END_FUZZIFY

FUZZIFY krvav ispljuvak
	TERM 
END_FUZZIFY

FUZZIFY suzenje
	TERM 
END_FUZZIFY

FUZZIFY promuklost
	TERM 
END_FUZZIFY

FUZZIFY hripanje
	TERM 
END_FUZZIFY

FUZZIFY zacepljen nos
	TERM 
END_FUZZIFY

FUZZIFY gubitak apetita
	TERM 
END_FUZZIFY

FUZZIFY slabost tijela
	TERM 
END_FUZZIFY


DEFUZZIFY decision
	TERM
	METHOD : COG;
	DEFAULT := 0;
END_DEFUZZIFY

