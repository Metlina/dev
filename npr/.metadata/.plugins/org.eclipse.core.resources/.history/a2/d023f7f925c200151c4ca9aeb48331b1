VAR_INPUT
	kihanje : REAL;
	kasljanje : REAL;
	grlobolja : REAL;
	otezano disanje : REAL;
	glavobolja : REAL;
	visina temperature : REAL;
	bol u plucima : REAL;
	krvav ispljuvak : REAL;
	suzenje : REAL;
	promuklost : REAL;
	hripanje : REAL;
	zacepljen nos : REAL;
	gubitak apetita : REAL;
	slabost tijela : REAL;
END_VAR

VAR_OUTPUT
	decision : REAL;
END_VAR

FUZZIFY interest
	TERM
END_FUZZIFY

DEFUZZIFY decision
	TERM
	METHOD : COG;
	DEFAULT := 0;
END_DEFUZZIFY

